    "prettier": "^2.8.0",
    "typescript": "*",
    "vite": "^3.2.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "importOrder": [
      "react",
      "^react(.*)$",
      "^redux-(.*)$",
      "^[^@.](.*)$",
      "^@react(.*)$",
      "^@reduxjs(.*)$",
      "^@mantine(.*)$",
      "^@/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true
  }
